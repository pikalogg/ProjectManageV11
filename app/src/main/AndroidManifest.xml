<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pikalong.projectmanagev11">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeployingTaskActivity"></activity>
        <activity android:name=".InspectTaskActivity" />
        <activity android:name=".SuccessfulTaskActivity" />
        <activity android:name=".GiveTaskActivity" />
        <activity android:name=".MessengerActivity" />
        <activity android:name=".AddTaskActivity" />
        <activity android:name=".AddProjectActivity" />
        <activity android:name=".ProjectActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>